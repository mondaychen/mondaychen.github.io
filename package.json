{
  "name": "personal-site-v2.5",
  "version": "1.0.0",
  "private": true,
  "description": "Monday Chen&#39;s personal website and blog",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && echo mengdi.dev > ./dist/CNAME",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "prepare": "husky install",
    "deploy": "push-dir --dir=dist --branch=master --cleanup"
  },
  "engines" : {
    "node" : ">=18.0.0 <19.0.0"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.**": "prettier --ignore-unknown"
  },
  "dependencies": {
    "core-js": "^3.25.3",
    "nuxt": "^2.15.8",
    "vue": "^2.7.10",
    "vue-material": "^1.0.0-beta-16",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^5.3.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "fibers": "^5.0.3",
    "frontmatter-markdown-loader": "^3.7.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.4.1"
  }
}
